{
  "name": "master-thesis/master-thesis-cms",
  "require": {
    "cweagans/composer-patches": "~1.0",
    "davidbarratt/custom-installer": "1.0.*@alpha",
    "composer/installers": "~1.0",
    "derhasi/composer-preserve-paths": "0.1.*",

    "ckeditor/ckeditor": "^4.4",

    "drupal/drupal": "^7.41",

    "drupal/admin_menu": "^7.3@rc",
    "drupal/ckeditor": "^7.1",
    "drupal/ctools": "^7.1",
    "drupal/devel": "^7.1",
    "drupal/diff": "^7.3",
    "drupal/environment_indicator": "^7.2",
    "drupal/features": "^7.2",
    "drupal/fpa": "^7.2",
    "drupal/master": "7.2.*@beta",
    "drupal/module_filter": "^7.2",
    "drupal/strongarm": "7.2.*",
    "drupal/views": "^7.3",
    "drupal/adminimal_theme": "^7.1",
    "drupal/ftools": "^7.1",
    "drupal/libraries": "^7.2",
    "drupal/entityreference": "^7.1",
    "drupal/bootstrap": "7.3.*",
    "drupal/double_field": "^7.2",
    "drupal/term_reference_tree": "^7.1",
    "drupal/field_group": "^7.1",
    "drupal/conditional_fields": "^7.3@alpha",
    "drupal/panels": "^7.3"
  },
  "scripts": {
    "post-create-project-cmd": ["rm README.md LICENSE .travis.yml phpunit.xml.dist CHANGELOG.txt .gitignore"]
  },
  "config": {
    "vendor-dir": "vendor"
  },
  "extra": {
    "custom-installer": {
      "drupal-core": "www/"
    },
    "installer-paths": {
      "www/": ["type:drupal-core"],
      "www/sites/all/modules/contrib/{$name}/": ["type:drupal-module"],
      "www/sites/all/themes/contrib/{$name}/": ["type:drupal-theme"],
      "www/sites/all/libraries/{$name}/": ["type:drupal-library"],
      "www/sites/all/drush/{$name}/": ["type:drupal-drush"],
      "www/profiles/{$name}/": ["type:drupal-profile"]
    },
    "preserve-paths": ["www/robots.txt", "www/files", "www/sites/default", "www/sites/all"]
  },
  "repositories": [{
    "type": "composer",
    "url": "http://packagist.drupal-composer.org"
  }, {
    "type": "package",
    "package": {
      "name": "ckeditor/ckeditor",
      "version": "4.4.7",
      "type": "drupal-library",
      "source": {
        "type": "git",
        "url": "https://github.com/ckeditor/ckeditor-releases.git",
        "reference": "cc324415302784eb1abe33a3762fbdd3b48c558b"
      },
      "dist": {
        "type": "zip",
        "url": "https://api.github.com/repos/ckeditor/ckeditor-releases/zipball/cc324415302784eb1abe33a3762fbdd3b48c558b",
        "reference": "cc324415302784eb1abe33a3762fbdd3b48c558b",
        "shasum": ""
      },
      "notification-url": "https://packagist.org/downloads/"
    }
  }]
}
